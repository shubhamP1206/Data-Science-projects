    {
      "cell_type": "code",
      "source": [
        "print(\"MULTIPLICATION TABLE\")\n",
        "print(\"*\"* 50)\n",
        "\n",
        "a = int(input(\"Enter the multiple \"))\n",
        "i = 1 \n",
        "\n",
        "while(i<=10):\n",
        "   print(a,\"x\",i,\"=\",a*i)\n",
        "\n",
        "   i = i+1\n",
        "   "
