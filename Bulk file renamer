import os

def renamer():
  i = 0
  path = "D:\\data science\\college finder\\projects\\bulk rename\\img\\"
  for filename in os.listdir(path):
      names = f"picture {1}.jpeg"
      src = path + filename
      dest = path + names

      os.rename(src,dest)
      i = i+1

  if __name__ == "__main__":
    renamer()
